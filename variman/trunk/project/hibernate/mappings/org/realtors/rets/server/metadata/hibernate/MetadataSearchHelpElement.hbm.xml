<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  
  <class name="org.nar.rets.metadata.hibernate.MetadataSearchHelpElement" table="e_search_help">
  
  
    <id column="id" name="uid" type="long">
      <generator class="hilo"/>
    </id>
    
    <property column="search_help_id" name="searchHelpID" type="string"/>
    <property column="value" name="value" type="string"/>

  </class>
  
</hibernate-mapping>
