<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: RETSDataElement.hbm.xml,v 1.2 2003/06/12 20:55:44 kgarner Exp $ -->

<hibernate-mapping>

  <class name="org.realtors.rets.server.data.RETSDataElement"
         table="rets_data_dataelement">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <many-to-one name="data" class="org.realtors.rets.server.data.RETSData"/>

    <many-to-one name="key" cascade="none" unique="true" not-null="true"
                 class="org.realtors.rets.server.metadata.MetadataTable"/>

    <property name="booleanValue" type="java.lang.Boolean"/>

    <property name="characterValue" type="string"/>

    <property name="dateValue" type="date"/>

    <property name="dateTimeValue" type="timestamp"/>

    <property name="timeValue" type="time"/>

    <property name="intValue" type="java.lang.Long"/>

    <property name="decimalValue" type="big_decimal"/>

    <many-to-one name="multi"
                 class="org.realtors.rets.server.data.RETSMultiSet"/>

  </class>
</hibernate-mapping>
