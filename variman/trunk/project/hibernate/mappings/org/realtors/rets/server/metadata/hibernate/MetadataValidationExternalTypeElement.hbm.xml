<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  
  <class name="org.nar.rets.metadata.hibernate.MetadataValidationExternalTypeElement" table="e_validation_external_type">
  
    <!--<jcs-cache usage="read-write"/>-->
    <id name="id" column="uid" type="long">
      <generator class="hilo.long"/>
    </id>  

    <property column="search_field" name="searchField" type="string"/>
    <property column="display_field" name="displayField" type="string"/>
    <property column="result_fields" name="resultFields" type="string"/>

  </class>
  
</hibernate-mapping>
