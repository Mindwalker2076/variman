<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: MetadataValidationLookupType.hbm.xml,v 1.1 2003/06/02 21:48:13 kgarner Exp $ -->

<hibernate-mapping>

  <class name="org.nar.rets.metadata.newdb.MetadataValidationLookupType"
    table="rets_metadata_validationlookuptype">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <property name="validText" type="string">
      <column name="valid_text" length="32"/>
    </property>

    <property name="parent1Value" type="string">
      <column name="parent1value" length="32"/>
    </property>

    <property name="parent2Value" type="string">
      <column name="parent2value" length="32"/>
    </property>

  </class>
</hibernate-mapping>


