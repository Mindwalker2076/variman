<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: RETSMultiSet.hbm.xml,v 1.6 2003/06/19 20:20:52 kgarner Exp $ -->

<hibernate-mapping>
  <class name="org.realtors.rets.server.data.RETSMultiSet"
         table="rets_multi_data">
    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <many-to-one name="parent" cascade="none"
                 class="org.realtors.rets.server.data.RETSDataElement">
      <column name="parent" index="rets_multi_data_parent"/>
    </many-to-one>

    <set name="values" table="rets_multi_data_set">
      <key column="id"/>
      <many-to-many column="value"
                 class="org.realtors.rets.server.metadata.LookupType"/>
    </set>
  </class>
</hibernate-mapping>
