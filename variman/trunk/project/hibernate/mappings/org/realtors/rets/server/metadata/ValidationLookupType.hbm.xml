<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: ValidationLookupType.hbm.xml,v 1.4 2003/06/19 20:20:52 kgarner Exp $ -->

<hibernate-mapping>

  <class name="org.realtors.rets.server.metadata.ValidationLookupType"
         table="rets_metadata_validationlookuptype">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <many-to-one name="validationLookupID" cascade="none"
           class="org.realtors.rets.server.metadata.ValidationLookup"/>

    <property name="validText" type="string">
      <column name="valid_text" length="32"/>
    </property>

    <property name="parent1Value" type="string">
      <column name="parent1value" length="32"/>
    </property>

    <property name="parent2Value" type="string">
      <column name="parent2value" length="32"/>
    </property>

  </class>
</hibernate-mapping>


