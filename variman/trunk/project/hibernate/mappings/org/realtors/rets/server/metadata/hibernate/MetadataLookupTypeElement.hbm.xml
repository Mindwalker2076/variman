<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  
  <class name="org.nar.rets.metadata.hibernate.MetadataLookupTypeElement" table="e_lookup_type">
  
    <id name="id" column="uid" type="long">
      <generator class="hilo"/>
    </id>  

    <property column="long_value" name="longValue" type="string"/>
    <property column="short_value" name="shortValue" type="string"/>
    <property column="value" name="value" type="string"/>

  </class>
  
</hibernate-mapping>
