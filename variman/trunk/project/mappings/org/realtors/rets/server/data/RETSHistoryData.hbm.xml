<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: RETSHistoryData.hbm.xml,v 1.4 2003/06/09 18:06:18 kgarner Exp $ -->

<hibernate-mapping>
  <class name="org.nar.rets.data.RETSHistoryData" table="rets_history_data">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <many-to-one name="originalData" cascade="none"
      class="org.nar.rets.data.RETSData"/>

    <property name="dateChanged" type="date"/>

    <map name="dataElements" lazy="false" cascade="all">
      <key column="data"/>
      <index-many-to-many column="key"
        class="org.nar.rets.metadata.newdb.MetadataTable"/>
      <one-to-many class="org.nar.rets.data.RETSHistoryDataElement"/>
    </map>
    
  </class>
</hibernate-mapping>
