<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: MetadataLookupType.hbm.xml,v 1.1 2003/06/02 21:48:13 kgarner Exp $ -->

<hibernate-mapping>

  <class name="org.nar.rets.metadata.newdb.MetadataLookupType"
    table="rets_metadata_lookuptype">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <property name="longValue" type="string">
      <column name="long_value" length="32"/>
    </property>

    <property name="shortValue" type="string">
      <column name="short_value" length="32"/>
    </property>

    <property name="value" type="string">
      <column name="value" length="32"/>
    </property>

  </class>
</hibernate-mapping>
