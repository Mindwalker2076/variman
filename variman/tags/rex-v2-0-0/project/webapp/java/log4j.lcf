log4j.rootLogger=warn, stdout
log4j.logger.net.sf.hibernate.util.JDBCExceptionReporter = off
log4j.logger.org.realtors.rets.server = info
#log4j.logger.org.realtors.rets.server = debug

### direct log messages to stdout ###
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.Target=System.err
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%5r %-5p [%t] %c{2} - %m%n
