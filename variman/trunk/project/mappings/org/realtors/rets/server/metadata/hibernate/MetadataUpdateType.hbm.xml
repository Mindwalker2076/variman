<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
  
  <class name="org.nar.rets.metadata.hibernate.MetadataUpdateType" table="m_update_type">
  
    <id name="id" column="uid" type="long">
      <generator class="hilo"/>
    </id> 
 
    <many-to-one name="stamp" class="org.nar.rets.metadata.hibernate.MetadataStamp" cascade="all"/>

    <set name="elements" lazy="false" cascade="all">
      <key column="elements"/>
      <one-to-many class="org.nar.rets.metadata.hibernate.MetadataUpdateTypeElement"/>
    </set>
    
  </class>
  
</hibernate-mapping>
