<?xml version="1.0" ?>
<object-set
   xmlns="http://www.crt.realtors.org/xml/variman/object-set"
   resource-key="abc123"
   base-path="http://example.com/objects/">
  <object-group type="Photo">
    <object>
      <file>abc123-1.jpg</file>
      <description>Beautiful frontal view of home.</description>
    </object>
    <object default="true">
      <file>abc123-2.jpg</file>
      <description/>
    </object>
    <object>
      <file>abc123-1.gif</file>
    </object>
  </object-group>

  <object-group type="Documents" base-path="../dirTest/">
    <object>
      <file mimetype="">foo.xml</file>
      <description>Some basic document.</description>
    </object>
    <object default="true">
      <file mimetype="">a/bar.html</file>
      <description>Disclosure statement from current owners.</description>
    </object>
    <object>
      <file mimetype="">2002 tax records.rtf</file>
      <description>County tax records for 2002.</description>
    </object>
  </object-group>

  <object-group type="Video" base-path="other/">
    <object>
      <file mimetype="">653083_virtual_tour.mov</file>
      <description>Take a virtual tour of this house.</description>
    </object>
  </object-group>
</object-set>
