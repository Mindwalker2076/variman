<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: RETSHistoryMultiSet.hbm.xml,v 1.1 2003/06/09 18:06:37 kgarner Exp $ -->

<hibernate-mapping>
  <class name="org.nar.rets.data.RETSHistoryMultiSet"
    table="rets_history_multi_data">

    <id name="id" type="long">
      <generator class="hilo"/>
    </id>

    <many-to-one name="parent" cascade="none"
      class="org.nar.rets.data.RETSHistoryDataElement">
      <column name="parent" index="rets_history_multi_data_parent"/>
    </many-to-one>

    <set name="values" table="rets_history_multi_data_set">
      <key column="id"/>
      <!-- this should be a pointer to lookup type? -->
      <element column="value" type="string"/>
    </set>
  </class>
</hibernate-mapping>
